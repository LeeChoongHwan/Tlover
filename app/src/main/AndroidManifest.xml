<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.teamproject1">

    <!-- 인터넷 사용 권한 설정-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 저장소 접근 권한 설정-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- 카메라 접근 권한 설정-->
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:name=".util.TloverApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"

        android:theme="@style/AppTheme">
        <activity android:name=".id.view.activity.IntroActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstTitleActivity">
        </activity>
        <activity android:name=".id.view.activity.SignUpingActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".id.view.activity.CreateAccountActivity">
        </activity>
        <activity android:name=".id.view.activity.SignInActivity">
        </activity>
        <activity android:name=".id.view.activity.SelectDestActivity">
        </activity>
        <activity android:name=".id.view.activity.SelectThemeActivity">
        </activity>
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".id.view.activity.LoginSuccessActivity">
        </activity>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />


</manifest>